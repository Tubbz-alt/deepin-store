#!/usr/bin/make -f
export DH_VERBOSE=1
include debian/generate_mo.mk

%:
	dh ${@} --with python2

override_dh_auto_build: gen_locale_mo
	dh_auto_build

override_dh_install:
	dh_install
	rm -r debian/deepin-software-center/usr/share/deepin-software-center/pkg_manager/apt/dbus_script
	rm debian/deepin-software-center/usr/share/deepin-software-center/pkg_manager/apt/install.sh
